<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="admin" catalog="library"
		class="com.library.domain.Admin">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_book" catalog="library"
		class="com.library.domain.Book">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_booktype" catalog="library"
		class="com.library.domain.Booktype">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_loaninfo" catalog="library"
		class="com.library.domain.Loaninfo">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_reader" catalog="library"
		class="com.library.domain.Reader">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_readertpye" catalog="library"
		class="com.library.domain.Readertpye">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_customer" catalog="food"
		class="com.Food.model.Customer">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_food" catalog="food" class="com.Food.model.Food">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="t_order" catalog="food" class="com.Food.model.Order">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>